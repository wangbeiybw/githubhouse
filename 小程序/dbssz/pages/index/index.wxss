/**index.wxss**/
.viewmodel{padding: 20rpx 0 20rpx 0; }
.table {
  border: 0px solid darkgray;
  width:100%;
  height:100%;
 }
 .tr {
  display: flex;
  width: 100%;
  justify-content: center;
  height:70rpx;
  align-items: center;
 }
 .td {
  width:40%;
  justify-content: center;
  text-align: center;
 }
 .td:first-child{text-align: left;padding-left: 10rpx;}
 .bg-w{
  background: #fff;
 }
 .bg-g{
  background: #e9edf4;
 }
 .th {
  width: 40%;
  justify-content: center;
  background: #43a3f9;
  color: #fff;
  display: flex;
  height: 3rem;
  align-items: center;
 }

 .vselect{line-height: 55rpx;border: 1px solid #e6e6e6;
  border-radius: 10rpx;text-align: center;padding: 0 10rpx;width:150rpx;margin-right: 10rpx;}
 .quyu{
   position: absolute;top:62rpx;left:15rpx;background: white;
  border: 20rpx;padding: 15rpx;width:400rpx;border-radius:10rpx;
  border: 1px solid #e6e6e6;display: flex;flex-wrap: wrap; flex-direction: row;
z-index: 9999;}
 .quyuitem{width: 180rpx;display: flex; flex-direction: row; align-items: center; justify-content: space-between; line-height: 45rpx;}
 .quyuitem:nth-child(odd){margin-right:30rpx}
 .fa{margin-left:10rpx}
 .picker{width:100%;text-overflow: ellipsis; overflow: hidden;white-space:nowrap;}




 .searchmodel{position: relative;border-bottom:1rpx solid #e8e8e8; padding: 0 20rpx;}
 .searchmodel input{border-radius: 8rpx;background: #f4f4f4; text-align:left;padding: 10rpx 10rpx 10rpx 50rpx;margin-bottom: 20rpx;}
 .fa-search{position: absolute;top:17rpx ;left:35rpx;color:#a6a7a9}

 

